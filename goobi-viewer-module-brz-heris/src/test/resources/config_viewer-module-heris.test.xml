<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<enabled>true</enabled>
    
    <scheme>
       <propertyName>PORTAL-SCHEME-TEST</propertyName>
    </scheme>

    <authority propertyType="attribute">
        <propertyName>PORTAL-AUTHORITY-TEST</propertyName>
        <valueMappingList>
            <item authority="foo.example.com">bar.example.com</item>
        </valueMappingList>
    </authority>
    
    <urlPatterns>
        <pattern host="example.com" field="PI">{SCHEME}://{HOST}/{ID}</pattern>
    </urlPatterns>
</config>
